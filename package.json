{
  "name": "vite-project",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bun test && bun run build:clear && bun run build:client && bun run build:server && bun run build:server-bundle && bun run build:entry && bun run build:after",
    "build:after": "cp .env.example dist/.env && cp -r drizzle dist/drizzle",
    "build:clear": "rm -rf dist",
    "build:client": "bunx --bun vite build --outDir dist/client",
    "build:entry": "bun build src/server/entry/entry-prod.ts --outfile dist/main.js --target bun",
    "build:entry-compile": "bun build --compile dist/main.js --outfile dist/main --target bun",
    "build:server": "bunx --bun vite build --ssr src/entry-server.tsx --outDir dist/server",
    "build:server-bundle": "bun build dist/server/entry-server.js --outfile dist/server/entry-server-bundle.js --target bun",
    "build:standalone": "bun run build && bun run build:entry-compile && rm dist/main.js && zip -r dist.zip dist",
    "ci": "bun install --frozen-lockfile",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "dev": "bun --hot src/server/entry/entry-dev.ts",
    "lint": "biome check --write .",
    "lint:fix": "biome check --write --unsafe .",
    "preview": "bun run build && cp .env dist/.env && cp -r runtime dist/runtime && cd dist && bun main.js",
    "sort": "sort-package-json",
    "tsc": "tsc"
  },
