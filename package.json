{
  "name": "violetjs",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bun scripts/build.ts",
    "build:standalone": "bun scripts/standalone.ts",
    "ci": "bun install --frozen-lockfile",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "deploy:docker:down": "docker-compose down",
    "deploy:docker:up": "docker-compose up -d --build",
    "dev": "cross-env NODE_ENV=development bun --hot src/server/entry/entry-dev.ts",
    "dev:preview": "bun run build && cp .env dist/.env && cp -R runtime dist/runtime && cd dist && bun app.js",
    "i18n:compile": "lingui compile",
    "i18n:extract": "lingui extract",
    "lint": "bun run lint:format && bun run lint:type",
    "lint:fix": "biome check --write --unsafe .",
    "lint:format": "biome format --write .",
    "lint:type": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "tool:clean": "rm -rf dist",
    "tool:sort": "sort-package-json",
    "tool:zip": "bun scripts/zip-dist.js"
  },
